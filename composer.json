{
    "name": "luxutils/laravel-table-optimize",
    "description": "Package for performing OPTIMIZE queries from Laravel app",
    "keywords": ["laravel","optimize","vacuum"],
    "homepage": "https://github.com/luxutils/laravel-table-optimize",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saulius Kazokas",
            "email": "saulens22@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Luxutils\\Optimizer\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3.0",
        "illuminate/database": "^8.0",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload-dev":{
        "classmap": ["tests/"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
